# Frontend environment variables (do not commit real .env to source control)
# The API base URL of the backend (use one of these).
REACT_APP_API_BASE=http://localhost:3001
# Alternative name supported:
# REACT_APP_BACKEND_URL=http://localhost:3001

# Public site URL used by redirects (if needed)
REACT_APP_FRONTEND_URL=http://localhost:3000

# WebSocket base URL (if applicable / future use)
REACT_APP_WS_URL=

# Node env flags
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=true
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/healthz

# Feature flags and experiments (JSON or comma separated)
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
